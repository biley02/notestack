<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">

    <title>notestack | Add a note</title>
</head>
<style>
    body {
        margin: 0;
        display: flex;
        box-sizing: border-box;
        padding: 4em;
    }
    
    #frm {
        position: relative;
        top: 0%;
        bottom: 0%;
        left: 0%;
        right: 0%;
        padding-top: 2em;
    }
</style>

<body>



    <div class="container">
        <div class="column">
            <h2>Add notes here.</h2>

            <form action="/sem1" method="POST" id=frm name=frm>
                <div class="form-group">
                    <label for="sub_code">SUBJECT CODE</label>
                    <input type="text" class="form-control" name="doc[subject_code]" id="sub_code">
                </div>
                <div class="form-group ">
                    <label for="SEMESTER">Semester</label>
                    <input type="number" class="form-control" id="SEMESTER" placeholder="(1-8)" name="doc[semester]" required>
                </div>

                <div class="form-group">
                    <label for="author">Drive Link</label>
                    <input type="text" class="form-control" name="doc[pdfurl]" id="author">
                </div>
                <div class="form-group">
                    <label for="author">Body</label>
                    <input type="text" class="form-control" name="doc[subject_topic]" id="author">
                </div>
                <div class="form-group">
                    <label for="author">Date Uploaded (DD/MM/YYYY)</label>
                    <input type="text" class="form-control" name="doc[dateuploaded]" id="author">
                </div>

                <button type="submit" class="btn btn-primary">Submit</button>

            </form>
        </div>


    </div>

    </div>
    </div>



    <!-- Optional JavaScript -->
    <!-- jQuery first, then Popper.js, then Bootstrap JS -->
    <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
    <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
</body>

</html>